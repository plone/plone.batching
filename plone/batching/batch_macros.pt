<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      i18n:domain="plone"
>
  <body>

    <tal:macro metal:define-macro="navigation"
               tal:define="
                 batch batch|nothing;
                 batchformkeys batchformkeys|nothing;
               "
    >

      <tal:batchnavigation define="
                             batchnavigation nocall:context/@@batchnavigation;
                           "
                           replace="structure python:batchnavigation(batch, batchformkeys)"
      />

    </tal:macro>

  </body>
</html>
